    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "@types/prop-types": "15.5.5",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "16.0.2",
    "apollo": "^2.9.0",
    "graphql-tools": "^4.0.4",
    "nodemon": "^1.19.1",
    "react-test-renderer": "16.5.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "client:start": "react-app-rewired start",
    "client:build": "react-app-rewired build",
    "client:build:staging": "env-cmd -f .env.staging npm run build",
    "client:test": "react-app-rewired test",
    "client:eject": "react-scripts eject",
    "client:schema": "apollo service:download ./src/schema.json",
    "client:types": "apollo client:codegen ./src/__generated__/types.ts --includes=./src/**/*.tsx,./src/**/*.ts --addTypename --localSchemaFile=./src/schema.json --target=typescript --outputFlat",
    "client:pre": "yarn client:types",

    "server:start": "NODE_ENV=development TS_NODE_PROJECT=server/tsconfig.json nodemon -e ts,graphql --ignore server/_graphql/ -x node -r ts-node/register server/index.ts",
    "server:debug": "NODE_ENV=development TS_NODE_PROJECT=server/tsconfig.json nodemon -e ts,graphql --ignore server/_graphql/ -x node --inspect -r ts-node/register server/index.ts",
    "server:start:staging": "NODE_ENV=staging TS_NODE_PROJECT=server/tsconfig.json nodemon -e ts,graphql --ignore server/_graphql/ -x node -r ts-node/register server/index.ts",
    "server:debug:staging": "NODE_ENV=staging TS_NODE_PROJECT=server/tsconfig.json nodemon -e ts,graphql --ignore server _graphql/ -x node --inspect -r ts-node/register server/index.ts",
    "server:ide": "NODE_ENV=development TS_NODE_PROJECT=server/tsconfig.json node --inspect -r ts-node/register server/index.ts",
    "server:schema": "ts-node ./server/scripts/genJoined.ts && apollo service:download  ./../src/schema.json",
    "server:types": "graphql-codegen",
    "server:pre": "yarn server:schema && yarn server:types",
    "server:prod": "NODE_ENV=production ts-node server/index.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
