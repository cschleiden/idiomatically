{
  "name": "idiomatically-client",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@types/react-router-dom": "^4.3.3",
    "antd": "^3.19.3",
    "apollo-boost": "^0.3.1",
    "babel-plugin-import": "^1.12.0",
    "customize-cra": "^0.2.13",
    "env-cmd": "^10.0.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.6.1",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "twemoji-parser": "^12.0.0"
  },
  "devDependencies": {
    "@types/graphql": "14.0.2",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "@types/prop-types": "15.5.5",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "16.0.2",
    "apollo": "^2.9.0",
    "react-test-renderer": "16.5.1",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "schema": "apollo service:download ./src/schema.json",
    "types": "apollo client:codegen ./src/__generated__/types.ts --includes=./src/**/*.tsx,./src/**/*.ts --addTypename --localSchemaFile=./src/schema.json --target=typescript --outputFlat",
    "pre": "yarn types"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
