    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.12",
    "@types/graphql": "^14.0.2",
    "@types/mongodb": "^3.1.27",
    "@types/node": "10.9.4",
    "@types/passport": "^1.0.0",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "connect-ensure-login": "^0.1.1",
    "cors": "^2.8.5",
    "countries-list": "^2.4.2",
    "country-language": "^0.1.7",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "graphql": "^14.0.2",
    "memorystore": "^1.6.1",
    "merge-graphql-schemas": "^1.5.8",
    "mongodb": "^3.2.5",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "transliteration": "^2.1.3",
    "tslib": "^1.9.3",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.1.1",
    "@graphql-codegen/typescript": "^1.1.1",
    "@graphql-codegen/typescript-operations": "^1.1.1",
    "apollo": "^2.9.0",
    "graphql-tools": "^4.0.4",
    "nodemon": "^1.19.1",
    "ts-node": "^8.1.0"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon -e ts,graphql --ignore _graphql/ -x node -r ts-node/register index.ts",
    "debug": "NODE_ENV=development nodemon -e ts,graphql --ignore _graphql/ -x node --inspect -r ts-node/register index.ts",
    "start:staging": "NODE_ENV=staging nodemon -e ts,graphql --ignore _graphql/ -x node -r ts-node/register index.ts",
    "debug:staging": "NODE_ENV=staging nodemon -e ts,graphql --ignore _graphql/ -x node --inspect -r ts-node/register index.ts",
    "ide": "NODE_ENV=development node --inspect -r ts-node/register index.ts",
    "schema": "ts-node ./scripts/genJoined.ts && apollo service:download  ./../client/src/schema.json",
    "types": "graphql-codegen",
    "pre": "yarn schema && yarn types",
    "prod": "NODE_ENV=production ts-node index.ts"
  }
}
